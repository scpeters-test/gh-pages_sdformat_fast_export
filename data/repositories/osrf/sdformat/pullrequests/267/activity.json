ublic.atl-paas.netinitialsSP-1.png"}}, "nickname": "Steven Peters", "type": "user", "account_id": "557058:5de38267-b118-494c-aa76-4fab35448816"}}, "pull_request": {"type": "pullrequest", "id": 267, "links": {"self": {"href": "data/repositories/osrf/sdformat/pullrequests/267.json"}, "html": {"href": "#!/osrf/sdformat/pull-requests/267"}}, "title": "Fix cmake warnings on Windows"}}, {"comment": {"links": {"self": {"href": "data/repositories/osrf/sdformat/pullrequests/267/comments/16599027.json"}, "html": {"href": "#!/osrf/sdformat/pull-requests/267/_/diff#comment-16599027"}}, "deleted": false, "pullrequest": {"type": "pullrequest", "id": 267, "links": {"self": {"href": "data/repositories/osrf/sdformat/pullrequests/267.json"}, "html": {"href": "#!/osrf/sdformat/pull-requests/267"}}, "title": "Fix cmake warnings on Windows"}, "content": {"raw": "As far as I know the doubles quotes could help when the value of the variable contains whitespaces (Windows). Not sure if the `CMAKE_CXX_COMPILE_ID` can have whitespaces but is probably safer if we use Dirk's trick. 4ac80fc", "markup": "markdown", "html": "<p>As far as I know the doubles quotes could help when the value of the variable contains whitespaces (Windows). Not sure if the <code>CMAKE_CXX_COMPILE_ID</code> can have whitespaces but is probably safer if we use Dirk's trick. <a href=\"#!/osrf/sdformat/commits/4ac80fc\" rel=\"nofollow\" class=\"ap-connect-link\">4ac80fc</a></p>", "type": "rendered"}, "created_on": "2016-04-01T17:35:52.908529+00:00", "user": {"display_name": "Carlos Ag\u00fcero", "uuid": "{da8a8e89-4bb0-421b-bd0e-dbbed3d4ed6a}", "links": {"self": {"href": "https://api.bitbucket.org/2.0/users/%7Bda8a8e89-4bb0-421b-bd0e-dbbed3d4ed6a%7D"}, "html": {"href": "https://bitbucket.org/%7Bda8a8e89-4bb0-421b-bd0e-dbbed3d4ed6a%7D/"}, "avatar": {"href": "data/secure.gravatar.com/avatar/692bf15758111acaddae4da15a47f9e5d=httpsavatar-management--avatars.us-west-2.prod.public.atl-paas.netinitialsCA-0.png"}}, "nickname": "caguero", "type": "user", "account_id": "557058:4ded1ddf-947e-4154-bbd1-3dba24f1bdbd"}, "updated_on": "2016-04-01T17:35:52.916186+00:00", "type": "pullrequest_comment", "id": 16599027}, "pull_request": {"type": "pullrequest", "id": 267, "links": {"self": {"href": "data/repositories/osrf/sdformat/pullrequests/267.json"}, "html": {"href": "#!/osrf/sdformat/pull-requests/267"}}, "title": "Fix cmake warnings on Windows"}}, {"update": {"description": "", "title": "Fix cmake warnings on Windows", "destination": {"commit": {"hash": "f6e092825372", "type": "commit", "links": {"self": {"href": "data/repositories/osrf/sdformat/commit/f6e092825372.json"}, "html": {"href": "#!/osrf/sdformat/commits/f6e092825372"}}}, "repository": {"links": {"self": {"href": "data/repositories/osrf/sdformat.json"}, "html": {"href": "#!/osrf/sdformat"}, "avatar": {"href": "data/bytebucket.org/ravatar/{b6d52f9f-b070-41c0-807c-94af07ea375b}ts=1606789"}}, "type": "repository", "name": "sdformat", "full_name": "osrf/sdformat", "uuid": "{b6d52f9f-b070-41c0-807c-94af07ea375b}"}, "branch": {"name": "default"}}, "reason": "", "source": {"commit": {"hash": "4ac80fc1e762", "type": "commit", "links": {"self": {"href": "data/repositories/osrf/sdformat/commit/4ac80fc1e762.json"}, "html": {"href": "#!/osrf/sdformat/commits/4ac80fc1e762"}}}, "repository": {"links": {"self": {"href": "data/repositories/osrf/sdformat.json"}, "html": {"href": "#!/osrf/sdformat"}, "avatar": {"href": "data/bytebucket.org/ravatar/{b6d52f9f-b070-41c0-807c-94af07ea375b}ts=1606789"}}, "type": "repository", "name": "sdformat", "full_name": "osrf/sdformat", "uuid": "{b6d52f9f-b070-41c0-807c-94af07ea375b}"}, "branch": {"name": "fix_cmake_warnings"}}, "state": "OPEN", "author": {"display_name": "Carlos Ag\u00fcero", "uuid": "{da8a8e89-4bb0-421b-bd0e-dbbed3d4ed6a}", "links": {"self": {"href": "https://api.bitbucket.org/2.0/users/%7Bda8a8e89-4bb0-421b-bd0e-dbbed3d4ed6a%7D"}, "html": {"href": "https://bitbucket.org/%7Bda8a8e89-4bb0-421b-bd0e-dbbed3d4ed6a%7D/"}, "avatar": {"href": "data/secure.gravatar.com/avatar/692bf15758111acaddae4da15a47f9e5d=httpsavatar-management--avatars.us-west-2.prod.public.atl-paas.netinitialsCA-0.png"}}, "nickname": "caguero", "type": "user", "account_id": "557058:4ded1ddf-947e-4154-bbd1-3dba24f1bdbd"}, "date": "2016-04-01T17:35:20.969930+00:00"}, "pull_request": {"type": "pullrequest", "id": 267, "links": {"self": {"href": "data/repositories/osrf/sdformat/pullrequests/267.json"}, "html": {"href": "#!/osrf/sdformat/pull-requests/267"}}, "title": "Fix cmake warnings on Windows"}}, {"comment": {"links": {"self": {"href": "data/repositories/osrf/sdformat/pullrequests/267/comments/16597851.json"}, "html": {"href": "#!/osrf/sdformat/pull-requests/267/_/diff#comment-16597851"}}, "deleted": false, "pullrequest": {"type": "pullrequest", "id": 267, "links": {"self": {"href": "data/repositories/osrf/sdformat/pullrequests/267.json"}, "html": {"href": "#!/osrf/sdformat/pull-requests/267"}}, "title": "Fix cmake warnings on Windows"}, "content": {"raw": "@dirk-thomas used a different fix in catkin that involved adding a space character: https://github.com/ros/catkin/pull/731/files\n\n~~~\ndiff -r f6e092825372 cmake/DefaultCFlags.cmake\n--- a/cmake/DefaultCFlags.cmake\tMon Mar 28 12:03:43 2016 -0700\n+++ b/cmake/DefaultCFlags.cmake\tFri Apr 01 10:19:49 2016 -0700\n@@ -54,15 +54,15 @@\n endif()\n \n # Compiler-specific C++11 activation.\n-if (\"${CMAKE_CXX_COMPILER_ID}\" MATCHES \"GNU\")\n+if (\"${CMAKE_CXX_COMPILER_ID} \" MATCHES \"GNU \")\n     execute_process(\n         COMMAND ${CMAKE_CXX_COMPILER} -dumpversion OUTPUT_VARIABLE GCC_VERSION)\n     if (NOT (GCC_VERSION VERSION_GREATER 4.7))\n         message(FATAL_ERROR \"${PROJECT_NAME} requires g++ 4.8 or greater.\")\n     endif ()\n-elseif (\"${CMAKE_CXX_COMPILER_ID}\" MATCHES \"Clang\")\n+elseif (\"${CMAKE_CXX_COMPILER_ID} \" MATCHES \"Clang \")\n     set(CMAKE_CXX_FLAGS \"${CMAKE_CXX_FLAGS} -stdlib=libc++\")\n-elseif (\"${CMAKE_CXX_COMPILER_ID}\" STREQUAL \"MSVC\")\n+elseif (\"${CMAKE_CXX_COMPILER_ID} \" STREQUAL \"MSVC \")\n     if (MSVC_VERSION LESS 1800)\n         message(FATAL_ERROR \"${PROJECT_NAME} requires VS 2013 or greater.\")\n     endif()\n~~~", "markup": "markdown", "html": "<p>@dirk-thomas used a different fix in catkin that involved adding a space character: <a href=\"https://github.com/ros/catkin/pull/731/files\" rel=\"nofollow\" class=\"ap-connect-link\">https://github.com/ros/catkin/pull/731/files</a></p>\n<div class=\"codehilite\"><pre><span></span><span class=\"gh\">diff -r f6e092825372 cmake/DefaultCFlags.cmake</span>\n<span class=\"gd\">--- a/cmake/DefaultCFlags.cmake Mon Mar 28 12:03:43 2016 -0700</span>\n<span class=\"gi\">+++ b/cmake/DefaultCFlags.cmake Fri Apr 01 10:19:49 2016 -0700</span>\n<span class=\"gu\">@@ -54,15 +54,15 @@</span>\n endif()\n\n # Compiler-specific C++11 activation.\n<span class=\"gd\">-if (&quot;${CMAKE_CXX_COMPILER_ID}&quot; MATCHES &quot;GNU&quot;)</span>\n<span class=\"gi\">+if (&quot;${CMAKE_CXX_COMPILER_ID} &quot; MATCHES &quot;GNU &quot;)</span>\n     execute_process(\n         COMMAND ${CMAKE_CXX_COMPILER} -dumpversion OUTPUT_VARIABLE GCC_VERSION)\n     if (NOT (GCC_VERSION VERSION_GREATER 4.7))\n         message(FATAL_ERROR &quot;${PROJECT_NAME} requires g++ 4.8 or greater.&quot;)\n     endif ()\n<span class=\"gd\">-elseif (&quot;${CMAKE_CXX_COMPILER_ID}&quot; MATCHES &quot;Clang&quot;)</span>\n<span class=\"gi\">+elseif (&quot;${CMAKE_CXX_COMPILER_ID} &quot; MATCHES &quot;Clang &quot;)</span>\n     set(CMAKE_CXX_FLAGS &quot;${CMAKE_CXX_FLAGS} -stdlib=libc++&quot;)\n<span class=\"gd\">-elseif (&quot;${CMAKE_CXX_COMPILER_ID}&quot; STREQUAL &quot;MSVC&quot;)</span>\n<span class=\"gi\">+elseif (&quot;${CMAKE_CXX_COMPILER_ID} &quot; STREQUAL &quot;MSVC &quot;)</span>\n     if (MSVC_VERSION LESS 1800)\n         message(FATAL_ERROR &quot;${PROJECT_NAME} requires VS 2013 or greater.&quot;)\n     endif()\n</pre></div>", "type": "rendered"}, "created_on": "2016-04-01T17:20:04.754629+00:00", "user": {"display_name": "Steve Peters", "uuid": "{2ccfed09-18b8-4921-8d58-15ef01092802}", "links": {"self": {"href": "https://api.bitbucket.org/2.0/users/%7B2ccfed09-18b8-4921-8d58-15ef01092802%7D"}, "html": {"href": "https://bitbucket.org/%7B2ccfed09-18b8-4921-8d58-15ef01092802%7D/"}, "avatar": {"href": "data/secure.gravatar.com/avatar/1fb4816dad9e68337d3096f750951f6cd=httpsavatar-management--avatars.us-west-2.prod.public.atl-paas.netinitialsSP-1.png"}}, "nickname": "Steven Peters", "type": "user", "account_id": "557058:5de38267-b118-494c-aa76-4fab35448816"}, "updated_on": "2016-04-01T17:20:04.802110+00:00", "type": "pullrequest_comment", "id": 16597851}, "pull_request": {"type": "pullrequest", "id": 267, "links": {"self": {"href": "data/repositories/osrf/sdformat/pullrequests/267.json"}, "html": {"href": "#!/osrf/sdformat/pull-requests/267"}}, "title": "Fix cmake warnings on Windows"}}, {"update": {"description": "", "title": "Fix cmake warnings on Windows", "destination": {"commit": {"hash": "f6e092825372", "type": "commit", "links": {"self": {"href": "data/repositories/osrf/sdformat/commit/f6e092825372.json"}, "html": {"href": "#!/osrf/sdformat/commits/f6e092825372"}}}, "repository": {"links": {"self": {"href": "data/repositories/osrf/sdformat.json"}, "html": {"href": "#!/osrf/sdformat"}, "avatar": {"href": "data/bytebucket.org/ravatar/{b6d52f9f-b070-41c0-807c-94af07ea375b}ts=1606789"}}, "type": "repository", "name": "sdformat", "full_name": "osrf/sdformat", "uuid": "{b6d52f9f-b070-41c0-807c-94af07ea375b}"}, "branch": {"name": "default"}}, "reason": "", "source": {"commit": {"hash": "1928dec246e2", "type": "commit", "links": {"self": {"href": "data/repositories/osrf/sdformat/commit/1928dec246e2.json"}, "html": {"href": "#!/osrf/sdformat/commits/1928dec246e2"}}}, "repository": {"links": {"self": {"href": "data/repositories/osrf/sdformat.json"}, "html": {"href": "#!/osrf/sdformat"}, "avatar": {"href": "data/bytebucket.org/rava